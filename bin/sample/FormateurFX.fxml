<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FormateurFX">
    <AnchorPane id="sidebar" fx:id="sidebar" prefHeight="720.0" prefWidth="300.0">
        <children>
            <Button id="FormationButton" fx:id="FormationButton" layoutY="220.0" mnemonicParsing="false" onAction="#Formation" prefHeight="70.0" prefWidth="300.0" text="Formation">
            <font>
               <Font size="18.0" />
            </font></Button>
            <Button id="LogoutButton" fx:id="LogoutButton" layoutX="50.0" layoutY="550.0" mnemonicParsing="false" onAction="#logOut" prefHeight="50.0" prefWidth="200.0" text="Se Deconnecter">
            <font>
               <Font size="18.0" />
            </font></Button>
         <Button id="NotionButton" fx:id="NotionButton" layoutY="290.0" mnemonicParsing="false" onAction="#Notions" prefHeight="70.0" prefWidth="300.0" text="Notions">
            <font>
               <Font size="18.0" />
            </font>
         </Button>
         <Button id="QuizButton" fx:id="QuizButton" layoutY="360.0" mnemonicParsing="false" onAction="#Quizs" prefHeight="70.0" prefWidth="300.0" text="Quizs">
            <font>
               <Font size="18.0" />
            </font>
         </Button>
         <Button id="ApprenantButton" fx:id="ApprenantButton" layoutY="430.0" mnemonicParsing="false" onAction="#Apprenants" prefHeight="70.0" prefWidth="300.0" text="Apprenants">
            <font>
               <Font size="18.0" />
            </font>
         </Button>
         <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="50.0" layoutY="120.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@images/esiquiz.png" />
            </image>
         </ImageView>
        </children>
    </AnchorPane>
   <Pane id="pane" fx:id="pane" layoutX="300.0" prefHeight="720.0" prefWidth="980.0">
      <children>
         <Label layoutX="403.0" layoutY="115.0" text="Bienvenue à">
            <font>
               <Font size="42.0" />
            </font>
         </Label>
         <ImageView fitHeight="162.0" fitWidth="407.0" layoutX="320.0" layoutY="216.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@images/esiquiz2.png" />
            </image>
         </ImageView>
         <Button layoutX="399.0" layoutY="445.0" mnemonicParsing="false" onAction="#Notions" prefHeight="53.0" prefWidth="232.0" text="Créer des notions">
            <font>
               <Font size="20.0" />
            </font>
         </Button>
         <Button layoutX="399.0" layoutY="535.0" mnemonicParsing="false" onAction="#Quizs" prefHeight="53.0" prefWidth="232.0" text="Générer des quizs">
            <font>
               <Font size="20.0" />
            </font>
         </Button>
      </children></Pane>

</AnchorPane>
